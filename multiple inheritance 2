interface Teacher {
    void attack();
}

interface Student {
    void use();
}

interface Principal {
    void head();
}

class Study implements Teacher, Student {
    public void attack() {
        System.out.println("Study attacks with a rank.");
    }
    public void use() {
        System.out.println("Student uses a rank.");
    }
}

class Exam implements Teacher, Student, Principal {
    public void attack() {
        System.out.println("Exam attacks with a result.");
    }
    public void use() {
        System.out.println("Exam uses a result.");
    }
    public void head() {
        System.out.println("Result headed with marketing.");
    }
}

public class Main {
    public static void main(String[] args) {
        Study study = new Study();
        Exam exam = new Exam();
        study.attack();
        study.use();
        exam.attack();
        exam.use();
        exam.head();
    }
}
